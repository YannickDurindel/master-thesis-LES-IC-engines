@article{galganoNumericalSimulationGlowing2014,
  title = {Numerical Simulation of the Glowing Combustion of Moist Wood by Means of a Front‐based Model},
  author = {Galgano, Antonio and Di Blasi, Colomba and Ritondale, Sergio and Todisco, Andrea},
  date = {2014-10},
  journaltitle = {Fire and Materials},
  shortjournal = {Fire and Materials},
