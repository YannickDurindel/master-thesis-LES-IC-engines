@article{zhangEfficientPyrolysisModel2023,
  title = {Efficient {{Pyrolysis Model}} for {{Large Biomass Particles}} with {{Arbitrary Shapes Based}} on a {{Composed-Sphere Concept}} and {{Voronoi Tessellation}}},
  author = {Zhang, Runhui and Ku, Xiaoke and Lin, Jianzhong and Luo, Kun},
  date = {2023-07-20},
  journaltitle = {Energy \& Fuels},
  shortjournal = {Energy Fuels},
  volume = {37},
  number = {14},
  pages = {10493--10505},
  issn = {0887-0624, 1520-5029},
  doi = {10.1021/acs.energyfuels.3c01269},
  url = {https://pubs.acs.org/doi/10.1021/acs.energyfuels.3c01269},
  urldate = {2024-01-26},
  abstract = {In this work, an efficient three-dimensional pyrolysis model for large biomass particles with arbitrary shapes has been developed based on the composed-sphere concept and the Voronoi tessellation. The shrinkage of the parent particle is realized by the active and passive translations of each composed sub-sphere. Moreover, the radical Voronoi tessellation can help resolve the heat transfer between sub-spheres and the porosity inside the particle. Meanwhile, the anisotropies of heat transfer and particle shrinkage are solved by adopting the orientation-related thermal conductivity and shrinking factors. To comprehensively validate the model, six different particles and pyrolysis processes (i.e., two uniformly shrunk spherical particles, a non-uniformly shrunk cylindrical particle, a non-uniformly heated cubic particle, and two non-regular shaped particles) are simulated. The simulation results agree well with the available experimental data. In addition, the sensitivity analyses of three different parameters (i.e., shrinking threshold, thermal conductivity, and shrinking factor) are also conducted. The developed model can not only well characterize different-shaped particles but also provide more intra-particle details during the pyrolysis of large biomass particles.},
  langid = {english},
  file = {/home/lbernau/sciebo/masterThesis/00_Literature/Zotero/storage/I6ZNYBF5/zhangEfficientPyrolysisModel2023.pdf}
}

@article{zhangStudyCanopyEffect2020,
  title = {A Study of the Canopy Effect on Fire Regime Transition Using an Objectively Defined {{Byram}} Convective Number},
  author = {Zhang, Kai and Verma, Salman and Trouvé, Arnaud and Lamorlette, Aymeric},
  date = {2020-03},
  journaltitle = {Fire Safety Journal},
  shortjournal = {Fire Safety Journal},
  volume = {112},
  pages = {102950},
  issn = {03797112},
  doi = {10.1016/j.firesaf.2020.102950},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0379711219304497},
  urldate = {2023-11-07},
  abstract = {Forest fires can be classified into two regimes, depending on Byram convective number NC: a plume-dominated and wind-driven regime. This classification is often based on two-dimensional (2D) studies without considering flame front structures, and is associated with the change of governing heat transfer mechanism. Studies based on three-dimensional (3D) considerations suggest the dependency of the fire regime transition on canopy charac­ teristics CdLAI (CdαsσsHF=2) through which the atmosphere boundary layer (ABL) flow changes to mixing layer (ML) flow. Hence, the primary objective of the present study is to investigate the fire regime transition and its associated heat transfer mechanisms with the aid of drawing a dimensionless configuration space \{NC, CdLAI\}. This requires an objective definition of the Byram convective number; the 2 m or 10 m high open wind speed (U∞ ¼ U2 or U10) often used to define it being arbitrary. Therefore, the present study introduces a scaling method for the reference height (Yref ) at which the wind speed Uref is chosen to define a new Byram convective number NC’. A mathematical expression for Yref as a function of canopy/vegetation height is proposed for a small-scale, stationary fire. The new configuration space \{NC’, CdLAI\} is considered more suitable for investigating the fire regime transition.},
  langid = {english},
  file = {/home/lbernau/sciebo/masterThesis/00_Literature/Zotero/storage/CP9FP6CB/zhangStudyCanopyEffect2020.pdf}
}

