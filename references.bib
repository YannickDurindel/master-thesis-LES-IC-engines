@article{corbettaPyrolysisCentimeterScaleWoody2014,
  title = {Pyrolysis of {{Centimeter-Scale Woody Biomass Particles}}: {{Kinetic Modeling}} and {{Experimental Validation}}},
  shorttitle = {Pyrolysis of {{Centimeter-Scale Woody Biomass Particles}}},
  author = {Corbetta, Michele and Frassoldati, Alessio and Bennadji, Hayat and Smith, Krystle and Serapiglia, Michelle J. and Gauthier, Guillaume and Melkior, Thierry and Ranzi, Eliseo and Fisher, Elizabeth M.},
