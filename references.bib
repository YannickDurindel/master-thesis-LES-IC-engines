@article{wurzenbergerThermalConversionBiomass2002,
  title = {Thermal Conversion of Biomass: {{Comprehensive}} Reactor and Particle Modeling},
  shorttitle = {Thermal Conversion of Biomass},
  author = {Wurzenberger, Johann C. and Wallner, Susanne and Raupenstrauch, Harald and Khinast, Johannes G.},
  date = {2002-10},
  journaltitle = {AIChE Journal},
  shortjournal = {AIChE Journal},
  volume = {48},
  number = {10},
  pages = {2398--2411},
  issn = {0001-1541, 1547-5905},
  doi = {10.1002/aic.690481029},
  url = {https://aiche.onlinelibrary.wiley.com/doi/10.1002/aic.690481029},
  urldate = {2024-01-24},
  abstract = {Abstract             Thermal conversion of biomass is often carried out in packed‐bed furnaces. Optimization of thermal efficiency and furnace emissions is an important goal, which requires accurate understanding of all physical and chemical effects in the reactor. A combined transient single particle and fuel‐bed model is presented. The fuel‐bed model is discretized, and a representative particle is chosen and discretized in a radial direction at each grid point. Mass, momentum and energy balances are solved for the entire system. Drying is modeled using an equilibrium approach, and primary pyrolysis is described by independent parallel‐reactions. Secondary tar cracking, homogeneous gas reactions, and heterogeneous char reactions are modeled using kinetic data from literature. Simulations validated for single particles agree well with experimental studies. Simulation results for the combustion of a biomass bed are presented for one set of furnace conditions.},
  langid = {english},
  file = {/home/lbernau/sciebo/masterThesis/00_Literature/Zotero/storage/F95HNV6Y/wurzenbergerThermalConversionBiomass2002.pdf}
}

@article{xuSimulationsCouplingCombustion2021,
  title = {Simulations of the Coupling between Combustion and Radiation in a Turbulent Line Fire Using an Unsteady Flamelet Model},
  author = {Xu, Rui and Le, Van Minh and Marchand, Alexis and Verma, Salman and Rogaume, Thomas and Richard, Franck and Luche, Jocelyn and Trouvé, Arnaud},
  date = {2021-03},
  journaltitle = {Fire Safety Journal},
  shortjournal = {Fire Safety Journal},
  volume = {120},
  pages = {103101},
  issn = {03797112},
  doi = {10.1016/j.firesaf.2020.103101},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0379711220301168},
  urldate = {2023-11-07},
  abstract = {The general objective of this study is to evaluate the potential of advanced combustion and radiation models for large eddy simulations (LES) of fires. We adopt here an unsteady laminar flamelet model that includes: detailed information on combustion chemistry through a tabulated chemistry approach; a careful description of the combustion-radiation coupling (local radiation phenomena are treated by the flamelet solver while non-local radiation phenomena are treated by the LES solver through the radiative transfer equation); a description of subgrid-scale turbulence-radiation interactions; and a description of non-grey radiation effects (through a Weighted-Sum-of-Grey-Gases – WSGG model). The new flamelet-based combustion/radiation model is incor­ porated into the LES solver FireFOAM and is evaluated by comparisons with experimental data obtained in a turbulent line burner experiment previously studied at the University of Maryland. Comparisons between simulated and measured temperatures show relatively good agreement (and significant improvements compared to our previous work). In addition, comparisons between simulated and measured values of the global radiant fraction show that provided that the WSGG option is used, the flamelet model is capable of simulating changes in the flame radiative emissions that result from changes in the oxygen strength of the coflow.},
  langid = {english},
  file = {/home/lbernau/sciebo/masterThesis/00_Literature/Zotero/storage/9CC2J8RF/xuSimulationsCouplingCombustion2021.pdf}
}

