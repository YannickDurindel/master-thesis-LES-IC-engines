@inbook{ahmedLargeEddySimulations2022,
  title = {Large Eddy Simulations of the Structure of Spreading Line Fires at Flame Scale},
  booktitle = {Advances in {{Forest Fire Research}} 2022},
  author = {Ahmed, Mohamed M. and Trouvé, Arnaud and Forthofer, Jason M. and Finney, Mark A.},
  date = {2022},
  edition = {1.ª Edição},
  pages = {283--288},
  publisher = {Imprensa da Universidade de Coimbra},
  doi = {10.14195/978-989-26-2298-9_44},
  url = {http://books.uc.pt/chapter?chapter=978989262298944},
  urldate = {2023-11-07},
  abstract = {Our general objective in the present study is to develop tools to better describe the coupling between solid phase and gas phase processes that control the dynamics of flame spread in wildland fire problems. We focus on a modelling approach that resolves processes occurring at flame scales, i.e., the formation of flammable vapors from the biomass vegetation due to pyrolysis, the subsequent combustion of these fuel vapors with ambient air, the establishment of a turbulent flow because of heat release and buoyant acceleration, and the thermal feedback to the solid biomass through radiative and convective heat transfer. The modelling capability is based on a general-purpose Computational Fluid Dynamics (CFD) library called OpenFOAM and an in-house Lagrangian particle model that treats drying, thermal pyrolysis, oxidative pyrolysis and char oxidation using a one-dimensional porous medium formulation that allows descriptions of thermal degradation processes occurring during both flaming and smoldering combustion.},
  bookauthor = {Viegas, Domingos Xavier and Ribeiro, Luís Mário},
  isbn = {978-989-26-2298-9 978-989-26-2297-2},
  langid = {english},
  file = {/home/lbernau/sciebo/masterThesis/00_Literature/Zotero/storage/KNKNBW7C/ahmedLargeEddySimulations2022.pdf}
}

@article{ahmedSimulationsUnsteadyResponse2021,
  title = {Simulations of the Unsteady Response of Biomass Burning Particles Exposed to Oscillatory Heat Flux Conditions},
  author = {Ahmed, Mohamed Mohsen and Trouvé, Arnaud},
  date = {2021-03},
  journaltitle = {Fire Safety Journal},
  shortjournal = {Fire Safety Journal},
  volume = {120},
  pages = {103059},
  issn = {03797112},
  doi = {10.1016/j.firesaf.2020.103059},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0379711220301089},
  urldate = {2023-11-07},
  abstract = {Understanding the mechanisms responsible for the ignition and pyrolysis of natural vegetation fuel is crucial in modeling the rate of spread of wildland fires. This study provides a fundamental understanding of the burning process of biomass fuel particles under oscillatory heat flux conditions; the oscillations are studied as a model to represent flapping flames or fluctuations in the irradiation above natural vegetation beds. We consider two separate cases corresponding to unsteady radiative or convective heating, with oscillations at different fre­ quencies, and we consider charring (constant-volume) and non-charring (shrinking), elongated and squareshaped, solid fuel particles of 1 mm and 10 mm half-size. The results of the numerical simulations suggest that the particles experience a quasi-linear response in the case of fluctuating irradiation or fluctuating local gas temperature; this linear response means that the effects of oscillations can essentially be ignored. However, a non-linear response is obtained in the case of unsteady convective heating, when the local gas velocity fluctuates in phase with the local gas temperature; this non-linear response leads to a net effect of the oscillations in the form of an augmented heat transfer, higher temperatures, higher values of the fuel mass loss rate, and shorter burnout times.},
  langid = {english},
  file = {/home/lbernau/sciebo/masterThesis/00_Literature/Zotero/storage/7ITMSVY2/ahmedSimulationsUnsteadyResponse2021.pdf}
}

