@thesis{mellerLargeEddySimulation2024,
  title = {Large Eddy Simulation of Reactive Solid Fuel Systems Using Advanced Flamelet Approaches},
  author = {Meller, Dominik},
  date = {2024},
  institution = {Duisburg-Essen},
  location = {Duisburg},
  doi = {10.17185/duepublico/81596},
  url = {http://dx.doi.org/10.17185/duepublico/81596},
  urldate = {2023-07-11},
  abstract = {Fossil fuels, including coal, continue to dominate global energy consumption despite green initiatives. Recent disruptions in coal trade flows have led to increased usage in Europe. While efforts to transition to cleaner energy sources are expected to reduce coal demand, it is unlikely that coal usage will cease in the near future. Studying coal combustion is therefore crucial due to its significant environmental impact and carbon dioxide emissions. This work aims to enhance the understanding and optimization of coal combustion by developing an advanced large eddy simulation (LES) framework that integrates flamelet-based tabulation strategies within an Euler-Lagrange framework. This approach has the potential for wider applicability, extending beyond coal to other solid fuels such as iron. The findings of the published studies have made significant progress in this field. The investigations cover various flame setups, exploring coal combustion under different conditions. Through detailed LES and comparisons with experimental data, the studies demonstrate improved predictions of combustion characteristics, temperature profiles, species concentrations, and pollutant emissions. The first study focuses on a laboratory-scale coal jet flame, employing a four-dimensional flamelet/progress variable (FPV) approach, considering two mixture fractions for the hydrogen pilot and volatiles. The study highlights the importance of accounting for experimental artifacts and probe effects to improve the accuracy of simulations. It reveals significant deviations of up to 50 \% in the results due to the probing effect. This finding emphasizes the need to consider and correct for these effects when conducting numerical simulations of coal combustion. The second study investigates the co-firing of coal and ammonia, focusing on the same laboratoryscale coal jet flame. In this study, a five-dimensional three-mixture fraction FPV approach is used, additionally considering a mixture fraction for ammonia and developing a specific reaction mechanism for ammonia co-firing. The simulations show reasonable agreement with experimental data, improving predictions of OH and NH reaction zone locations. The third study examines a semi-industrial scale coal furnace with a low-NOx burner, using a four-dimensional FPV approach, considering two mixture fractions to account for volatile gases and char burnout to predict NOx emissions and understand formation mechanisms. The simulations show an overall good agreement with experimental observations, with the solution of an additional transport equation for NO proving to be superior in improving the prediction of forward- and backward reactions of NO within the furnace. These published papers advance our understanding of coal combustion dynamics, pollutant formation, and emission reduction strategies. The flamelet-based tabulation techniques and detailed reaction mechanisms employed in the studies enhance prediction accuracy and provide valuable insights into complex coal combustion processes.},
  langid = {english},
  pagetotal = {127},
  file = {/home/lbernau/sciebo/masterThesis/00_Literature/Zotero/storage/DMARLSIC/mellerLargeEddySimulation2023.pdf}
}

