@article{hasseAdvancedModelingApproaches2021,
  title = {Advanced Modeling Approaches for {{CFD}} Simulations of Coal Combustion and Gasification},
  author = {Hasse, Christian and Debiagi, Paulo and Wen, Xu and Hildebrandt, Klaus and Vascellari, Michele and Faravelli, Tiziano},
  date = {2021-09},
